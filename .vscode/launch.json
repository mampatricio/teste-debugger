{
    "version": "0.2.0",
    "configurations": [  
        {
            "type": "node",
            "request": "attach",
            "name": "Docker: Attach to Node",
            "sourceMaps": true,
            "smartStep": true,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/",
            "sourceMapPathOverrides": {
                "file://*": "${workspaceFolder}/*",
                "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
                "webpack:///./src/*": "${workspaceFolder}/*",
                "webpack:///./*": "${workspaceFolder}/*"
            }
      }
    ]
}
 